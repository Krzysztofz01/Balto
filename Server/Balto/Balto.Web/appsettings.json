{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\log.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "DatabaseSettings": {
    "SqlServerConnectionString": ""
  },
  "JsonWebTokenSettings": {
    "TokenSecret": "",
    "TokenExpirationInMinutes": "10"
  },
  "UserRoleNameSettings": {
    "Default": "Default",
    "Leader": "Leader"
  },
  "MonitoringSettings": {
    "AdvancedMonitoring": true
  },
  "EmailSmtpSettings": {
    "Host": "",
    "Port": "",
    "Address": "",
    "Login": "",
    "Password":  ""
  },
  "AllowedHosts": "*"
}
